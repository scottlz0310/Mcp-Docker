{
  "mcpServers": {
    "github": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-github",
        "/server/github-mcp-server",
        "stdio"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    }
  }
}
